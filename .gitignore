# Default .gitignore file

# Ignore build output
build/
dist/
bin/
out/
target/

# Ignore IDE and editor files
.vscode/
.idea/
cmake-build-debug/
*.sublime-project
*.sublime-workspace

# Ignore system and temporary files
.DS_Store
Thumbs.db
*.tmp
*.log

# Ignore dependency directories
node_modules/
vendor/

# Ignore package lock files
package-lock.json
yarn.lock

# Ignore compiled code and binaries
*.o
*.obj
*.exe
*.dll
*.so
*.dylib

# Ignore user-specific files
*.user
*.suo
*.DS_Store
*.cache
*.settings
*.sln.docstates

